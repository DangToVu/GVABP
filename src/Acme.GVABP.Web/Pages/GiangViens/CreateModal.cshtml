@page
@using Acme.GVABP.Localization
@using Acme.GVABP.Web.Pages.GiangViens
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<GVABPResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="GiangVien" asp-page="/GiangViens/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewGiangVien"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
